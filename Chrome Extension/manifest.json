{
  "name": "New tab page",
  "description": "Override the new tab page with a blank one",
  "version": "0.2",
  "incognito": "split",
  "permissions": ["tabs"],
  "chrome_url_overrides": {
    "newtab": "blank.html"
  },

  "browser_action": {
    "default_icon": "chrome-icono.png"
  },
  
  "manifest_version": 2
}
